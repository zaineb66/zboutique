
@{
    ViewBag.Title = "category_share";
    myDatabaseEntities md = new myDatabaseEntities();
}


<!DOCTYPE html>
<html lang="en">
<head>
    @using PROJECT.Models
    
    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts",required:false)

    <meta charset="utf-8">
    <title>nifty Boutique</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <!-- styles -->
    <script src="~/Scripts/jquery-1.8.2.min.js"></script>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <!--<link href="~/Content/bootstrap.css" rel="stylesheet" />-->
    <link href="~/Content/bootstrap-responsive.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-responsive.min.css" rel="stylesheet" />
    <link href="~/Content/font-awesome.css" rel="stylesheet" />
    <link href="~/Content/shop-homepage.css" rel="stylesheet" />
    <link href="~/Content/style.css" rel="stylesheet" />

   
    <link href="~/Scripts/source/helpers/jquery.fancybox-buttons.css" rel="stylesheet" />
    <link href="~/Scripts/source/helpers/jquery.fancybox-thumbs.css" rel="stylesheet" />
    <link href="~/Content/jquery.fancybox.css" rel="stylesheet" />


    <link href="~/Content/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <!-- jQuery -->
    <script src="~/Scripts/slider/jquery.js"></script>

    <script src="~/Scripts/js/bootstrap.min.js"></script>
    <!-- <script src="~/Scripts/js/bootstrap.js"></script>-->
    <script src="~/Scripts/js/jquery.easing-1.3.min.js"></script>
   <!-- <script src="~/Scripts/js/jquery.js"></script>-->
    <script src="~/Scripts/js/jquery.scrollTo-1.4.3.1-min.js"></script>
    <script src="~/Scripts/js/shop.js"></script>
    <script src="~/Scripts/jquery.js"></script>
    <script src="~/Scripts/js/bootstrap-dropdown.js"></script>


    <script src="~/Scripts/lib/jquery-1.10.1.min.js"></script>
    <script src="~/Scripts/lib/jquery-1.9.0.min.js"></script>
    <script src="~/Scripts/lib/jquery.mousewheel-3.0.6.pack.js"></script>
    <script src="~/Scripts/source/jquery.fancybox.js"></script>
    <script src="~/Scripts/source/jquery.fancybox.pack.js"></script>
    <script src="~/Scripts/source/helpers/jquery.fancybox-thumbs.js"></script>
    <script src="~/Scripts/source/helpers/jquery.fancybox-media.js"></script>
    <script src="~/Scripts/source/helpers/jquery.fancybox-buttons.js"></script>
</head>

<body >


    <!-- Navigation -->
    <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
        <div class="container">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>

            </div>
            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav pull-left">
                    
                    <li>
                        <a href="/Home/Index">Home</a>
                    </li>
                    <li>
                        <a href="/Home/About">About</a>
                    </li>
                    <li>
                        <a href="/Home/Contact">Contact Us</a>
                    </li>

                    <li >
                        
                        <div  style="margin-top:6px ; margin-left:10px">
                        <form @*action="/Home/Search"*@ class="navbar-search" >
                            <input type="text" name="search" placeholder="Search By Name" class="search-query" required ><span><button type="submit" class="shopBtn ">search</button></span>
                        </form>
                            </div>
                            
                   </li>
                   </ul>

                <ul class="nav navbar-nav pull-right">
                    <li>
                        <a href="/Home/Shopping">Shopping Cart</a>

                    </li>

                    @{if (Session["uname"] != null) { 
                          <li>
                              <a href="/Home/Logout">Logout</a>
                          </li>
                    }}
                     
                   @{if (Session["uname"] == null) { 
                    <li class="dropdown">
                        <a data-toggle="dropdown" class="dropdown-toggle" href="#"><span class="icon-lock"></span> Login <b class="caret"></b></a>
                        <div class="dropdown-menu">
                            <form action="/Home/LoginApprove" class="form-horizontal loginFrm">
                                <div class="control-group">
                                    <input type="text" class="span2" id="inputEmail" placeholder="Username" name="user">
                                </div>
                                <div class="control-group">
                                    <input type="password" class="span2" id="inputPassword" name="pass" placeholder="Password">
                                </div>
                               <br/>
                                <center>
                                <button type="submit" class="shopBtn ">Sign in</button>
                                 </center>
                            </form>
                        </div>
                    </li>
                    }}

                    @{if (Session["uname"] == null) {
                    <li>
                        <a href="/Home/Register">Register</a>
                    </li>
                    }}
                </ul>
            </div>
            <!-- /.navbar-collapse -->
        </div>
        <!-- /.container -->
    </nav>

    <!-------------------------------------------------------------------------------------------------------------------------------------->

    <div class="container">

        <header id="header">
            <div class="row">
                <div class="span4">
                    <a class="logo" href="/Home/Index">
                        <img src="~/images/logo.png" width="240" height="150" alt="">
                    </a>
                </div>
            </div>

        </header>
    </div>



    <!-- Page Content -->
    <div class="container">

        <div class="row">

            <div class="col-md-3">
                <h1 style=" font-family:'Segoe UI' ;color: #713a05">CATEGORIES</h1>
                <div class="list-group">
                   @foreach (category cat in md.categories.ToList())
                   {

                        <a href="/Home/CategoryView/@cat.Id" class="list-group-item"><h3>@cat.desc</h3></a>


                    }

                </div>
            </div>

            <div class="col-md-9">

                <div class="well np">
                    <div id="myCarousel" class="carousel slide homCar">
                        <div class="carousel-inner">
                            <div class="item">
                                <img style="width:100%" src="~/images/ch2.jpg" alt="">

                            </div>
                            <div class="item">
                                <img style="width:100%" src="~/images/ch4.jpg" alt="">

                            </div>
                            <div class="item active">
                                <img style="width:100%" src="~/images/d1.jpg" alt="">

                            </div>

                        </div>
                        <a class="left carousel-control" href="#myCarousel" data-slide="prev">&lsaquo;</a>
                        <a class="right carousel-control" href="#myCarousel" data-slide="next">&rsaquo;</a>
                    </div>
                </div>

            </div>
             </div>
        <div class="row">
        <div class="col-md-3">
            <div class="well well-small" >
                <form @*action=""*@ >
                   <h4> Search In Price Range</h4>
                    <br />
                    <input type="number" name="search2" min="1" required placeholder="From" class="search-query " style="width:80px; border:groove"> 
                    <input type="number" name="search3" min="1" required placeholder="To" class="search-query " style="width:80px; border:groove">
                    <br /><br />
                    <button type="submit" class="shopBtn ">search</button>
                </form>
                </div>
                <div class="well well-small alert alert-warning cntr">
                    <h2>Keep Your Place</h2>
                    <h2>
                        Up To Dated . <br><br>
                    </h2>
                </div>
                <div class="well well-small"><a href="#"><img src="~/images/p1.jpg " style="width:100%" alt="payment method paypal"></a></div>
            </div>
            <div class="col-md-9">
                @RenderBody()

            
            
                </div>
            </div>
        

        </div>
  

    <!--
    Footer
    -->
    <footer class="footer" style="margin-bottom:1px">
        <div class="row-fluid">
            <p class="pull-left">
                <a href="#"><img src="~/images/fb.png" style="width:50px" alt=""></a>
                <a href="#"><img src="~/images/twitter.png" style="width:50px" alt=""></a>
                <a href="#"><img src="~/images/insta.png" style="width:50px" alt=""></a>
            </p>
            <p class="pull-right">
                <a href="#"><img src="~/images/maestro.png" alt="payment"></a>
                <a href="#"><img src="~/images/mc.png" alt="payment"></a>
                <a href="#"><img src="~/images/pp.png" alt="payment"></a>
                <a href="#"><img src="~/images/visa.png" alt="payment"></a>
                <a href="#"><img src="~/images/disc.png" alt="payment"></a>
            </p>

            
        </div>
    </footer>

           
</body>
</html>
